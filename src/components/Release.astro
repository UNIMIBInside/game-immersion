---

type Props = {
  title: string;
  version?: string;
  link?: string;
};

const { title, version, link } = Astro.props;
---

<li class="mb-20 ms-12">
  <div
    class="absolute w-3 h-3 bg-purple-500 rounded-full mt-1.5 -start-1.5 border border-purple-500"
  >
  </div>
  <div class="pt-1.5 mb-4 text-lg uppercase leading-none text-gray-500">
    {title}
  </div>
  <div class="space-y-12">
    <slot />
  </div>
</li>